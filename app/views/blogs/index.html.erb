<% @blogs.each do |blog| %>
  <div class="container">
    <div class="jumbotron text-center">
      <h4><%= blog.created_at.strftime("%Y/%m/%d") %></h4>
      <h2><b><%= blog.title if blog.title? %></b></h1>
      <%= image_tag blog.picture.url if blog.picture? %>
      <h4><%= blog.content %></h4>
    </div>
  </div>
<% end %>

<%= will_paginate @blogs, :previous_label => '&lt 前のページ', :next_label => '次のページ &gt' %>